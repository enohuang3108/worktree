{
  "name": "worktree-cli",
  "version": "1.0.0",
  "description": "CLI tool for automated git worktree management",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "wt": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "pkg": "pkg ."
  },
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^9.0.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "pkg": "^5.8.1"
  },
  "keywords": ["git", "worktree", "cli", "development"],
  "author": "",
  "license": "MIT",
  "pkg": {
    "targets": ["node18-linux-x64", "node18-macos-x64", "node18-win-x64"],
    "outputPath": "build"
  }
}